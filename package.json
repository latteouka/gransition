{
  "name": "@chundev/gtranz",
  "description": "Page transition context with GSAP timeline.",
  "version": "1.0.20",
  "author": "latteouka",
  "license": "ISC",
  "keywords": [
    "react",
    "typescript",
    "nextjs",
    "transition"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/latteouka/gtranz"
  },
  "scripts": {
    "build": "tsup"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "treeshake": true,
    "sourcemap": "inline",
    "minify": true,
    "clean": true,
    "dts": true,
    "splitting": false,
    "format": [
      "cjs",
      "esm"
    ],
    "external": [
      "react"
    ]
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "peerDependencies": {
    "gsap": ">=3",
    "react": ">=18"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@types/react": "^18.0.37",
    "esbuild-plugin-babel": "^0.2.3",
    "ts-loader": "^9.4.2",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  }
}
