{
  "name": "@chundev/gtranz",
  "version": "1.0.14",
  "type": "module",
  "description": "Page transition context with GSAP timeline.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": [
      "./*",
      "./*.d.ts"
    ]
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "global.d.ts",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/latteouka/gtranz"
  },
  "author": "latteouka",
  "license": "ISC",
  "dependencies": {
    "gsap": ">=3",
    "react": ">=18"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@types/react": "^18.0.37",
    "esbuild-plugin-babel": "^0.2.3",
    "ts-loader": "^9.4.2",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "keywords": [
    "react",
    "typescript",
    "nextjs",
    "transition"
  ]
